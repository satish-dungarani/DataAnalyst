<!-- Main row -->
<div class="row">
    <section class="col-lg-12 connectedSortable">
        <div class="row">
            <div class="col-md-12">
                <!-- Custom Tabs -->
                <div class="nav-tabs-custom">
                    <ul class="nav nav-tabs">
                        <li class="active"><a href="#Spend" data-toggle="tab">Spend</a></li>
                        <li class=""><a href="#Rebate" data-toggle="tab">Rebate</a></li>
                    </ul>
                    <div class="tab-content">
                        <div class="tab-pane active" id="Spend">
                            <div class="box-body">
                                <div id="SSMSLineChart" style="min-width: 800px; height: 520px; margin: 0 auto"></div>
                            </div>
                        </div><!-- /.tab-pane -->
                        <div class="tab-pane" id="Rebate">
                            <div class="box-body">
                                <div id="SSMRLineChart" style="min-width: 800px; height: 400px; margin: 0 auto"></div>
                            </div>
                        </div><!-- /.tab-pane -->

                    </div><!-- /.tab-content -->
                </div><!-- nav-tabs-custom -->
            </div>
        </div>

    </section><!-- /.Left col -->
    <!-- Left col -->
    <section class="col-lg-7 connectedSortable">

        <!-- quick email widget -->
        <div class="box box-info">
            <div class="box-header">
                <i class="fa  fa-bar-chart"></i>
                <h3 class="box-title">Supplier Sales Spend and Rebate</h3>
                <!-- tools box -->
                <div class="pull-right box-tools">
                    @*<button class="btn btn-info btn-sm pull-right" data-widget="collapse" data-toggle="tooltip" title="Collapse" style="margin-right: 5px;"><i class="fa fa-navicon"></i></button>*@
                </div><!-- /. tools -->
            </div>
            <div class="box-body">
                <div id="SMPLineChart" style="max-width: 800px; height: 300px; margin: 0 auto"></div>
            </div>
        </div>

    </section><!-- /.Left col -->
    <!-- right col (We are only adding the ID to make the widgets sortable)-->
    <section class="col-lg-5 connectedSortable">
        <div class="box box-info">
            <div class="box-header">
                <i class="fa  fa-pie-chart"></i>
                <h3 class="box-title">Supplier Rebate %</h3>
                <!-- tools box -->
                <div class="pull-right box-tools">
                    @*<button class="btn btn-info btn-sm pull-right" data-widget="collapse" data-toggle="tooltip" title="Collapse" style="margin-right: 5px;"><i class="fa fa-minus"></i></button>*@
                </div><!-- /. tools -->
            </div>
            <div class="box-body">
                <div id="SMPPieChart" style="max-width: 800px; height: 300px; margin: 0 auto"></div>
            </div>

        </div>
    </section><!-- right col -->

    <!--Pharmacy Purchase Bas on Supplier-->
    <section class="col-lg-12 connectedSortable">

        <!-- quick email widget -->
        <div class="box box-info">
            <div class="box-header">
                <i class="fa  fa-bar-chart"></i>
                <h3 class="box-title">Pharmacy Purchase Spend and Rebate [Top 20]</h3>
                <!-- tools box -->
                <div class="pull-right box-tools">
                    @*<button class="btn btn-info btn-sm pull-right" data-widget="collapse" data-toggle="tooltip" title="Collapse" style="margin-right: 5px;"><i class="fa fa-navicon"></i></button>*@
                </div><!-- /. tools -->
            </div>
            <div class="box-body">
                <div id="PBSLineChart" style="min-width: 800px; height: 300px; margin: 0 auto"></div>
            </div>
        </div>

    </section><!-- /.Left col -->
